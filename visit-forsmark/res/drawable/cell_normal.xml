<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    
	 <!-- pressed -->
    <item android:state_pressed="true">
        <layer-list>
            <item>
                <nine-patch android:src="@drawable/cell_normalbg_pressed"/>
            </item>
            <item>
                <shape>
                    
                     <gradient android:angle="90" android:endColor="#803899ff" android:centerColor="#135aa5"  android:startColor="#3899ff" />
                </shape>
            </item>
        </layer-list>
   </item>
   
    <item >
     	<layer-list>
            <item>
                <nine-patch android:src="@drawable/cell_normalbg" />
            </item>
            <item>
                <shape>
                    
                     <gradient android:angle="90" android:endColor="#800066d2" android:centerColor="#135aa5"  android:startColor="#0066d2" />
                </shape>
            </item>
        </layer-list>
    </item>
    
</selector>

