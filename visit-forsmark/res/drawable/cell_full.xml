<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    
	 <!-- pressed -->
    <item android:state_pressed="true">
        <layer-list>
            <item>
                <nine-patch android:src="@drawable/cell_normalbg_pressed"/>
            </item>
            <item>
                <shape>
                     <gradient android:angle="90" android:endColor="#80dedede" android:centerColor="#a5a5a5"  android:startColor="#EEEEEE" />
                </shape>
            </item>
        </layer-list>
   </item>
   
    <item >
     	<layer-list>
            <item>
                <nine-patch android:src="@drawable/cell_normalbg" />
            </item>
            <item>
                <shape>
                     <gradient android:angle="90" android:endColor="#80d4d4d4" android:centerColor="#a5a5a5"  android:startColor="#d4d4d4" />
                </shape>
            </item>
        </layer-list>
    </item>
    
</selector>

